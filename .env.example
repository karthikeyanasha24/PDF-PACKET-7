# Environment variables for PDF Packet Builder

# Cloudflare Worker URL for PDF processing
VITE_WORKER_URL=https://pdf-packet-generator.your-subdomain.workers.dev

# For local development, use:
# VITE_WORKER_URL=http://localhost:8787

# Optional: Analytics or tracking
# VITE_ANALYTICS_ID=your-analytics-id
VITE_CLOUDFLARE_ACCOUNT_ID=your_account_id

# Feature Flags
VITE_ENABLE_PREVIEW=true
VITE_ENABLE_ANALYTICS=false
VITE_DEBUG_MODE=true

# PDF Storage
VITE_PDF_STORAGE_URL=https://your-r2-bucket.your-subdomain.r2.dev
